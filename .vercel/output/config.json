{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/accounts/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/cron/refresh/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/ddragon/bootstrap/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/key-status/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/live-game/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/player-summaries/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/player-summary/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/resolve/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/search/?$",
			"dest": "_render"
		},
		{
			"src": "^/game/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/search/?$",
			"dest": "_render"
		},
		{
			"src": "^/$",
			"dest": "_render"
		}
	]
}